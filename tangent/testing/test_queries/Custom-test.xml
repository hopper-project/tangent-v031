	<topic>
		<num>Custom-test-5</num>
		<query>
		    <formula id="f1.0">
			
			<m:math alttext="a^2+b^2=c^2" display="inline">
		<m:mrow>
			<m:msup>
				<m:mi>a</m:mi>
				<m:mn>2</m:mn>
			</m:msup>
			<m:mo>+</m:mo>
			<m:msup>
				<m:mi>b</m:mi>
				<m:mn>2</m:mn>
			</m:msup>
			<m:mo>=</m:mo>
			<m:msup>
				<m:mi>c</m:mi>
				<m:mn>2</m:mn>
			</m:msup>
		</m:mrow>
	</m:math>
			
			</formula>
