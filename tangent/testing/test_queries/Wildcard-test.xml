	<topic>
		<num>Wildcard-test-14</num>
		<query>
		  <formula id="f1.0">
	<m:math alttext="S(t)=\frac{1}{?x0}" display="inline">
		<m:mrow>
			<m:mi>S</m:mi>
			<m:mrow>
				<m:mo>(</m:mo>
				<m:mi>t</m:mi>				  
				<m:mo>)</m:mo>
			</m:mrow>
			<m:mo>=</m:mo>
			<m:mfrac>
				<m:mrow>
					<m:mn>1</m:mn>
				</m:mrow>
				<m:mrow>
					<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x0"/>
				</m:mrow>
			</m:mfrac>
		</m:mrow>
	</m:math>
		  </formula>
		</query>
	</topic>
	<topic>
		<num>Wildcard-test-15</num>
		<query>
		  <formula id="f1.0">
	<m:math alttext="1+x+\frac{x^2}{2}+?x0+O(?x1)" display="inline">
		<m:mrow>
			<m:mn>1</m:mn>
			<m:mo>+</m:mo>
			<m:mi>x</m:mi>
			<m:mo>+</m:mo>
			<m:mfrac>
				<m:mrow>
					<m:msup>
						<m:mi>x</m:mi>
						<m:mn>2</m:mn>
					</m:msup>					
				</m:mrow>
				<m:mrow>
					<m:mn>2</m:mn>
				</m:mrow>
			</m:mfrac>
			<m:mo>+</m:mo>
			<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x0"/>
			<m:mo>+</m:mo>
			<m:mi>O</m:mi>
			<m:mrow>
				<m:mo>(</m:mo>
				<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x1"/>				  
				<m:mo>)</m:mo>
			</m:mrow>
		</m:mrow>
	</m:math>	
		  </formula>
		</query>
	</topic>
