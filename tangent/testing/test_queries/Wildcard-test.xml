<topics xmlns="http://ntcir-math.nii.ac.jp/" xmlns:m="http://www.w3.org/1998/Math/MathML">
	<topic>
		<num>Wildcard-test-1</num>
		<query>
		  <formula id="f1.0">
	<m:math alttext="?x0^2+1" display="inline">
		<m:mrow>
		  <m:msup>
			<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x0"/>
			<m:mn>2</m:mn>
		  </m:msup>
		  <m:mo>+</m:mo>
		  <m:mn>1</m:mn>
		</m:mrow>
	</m:math>
		  </formula>
		</query>
	</topic>
	<topic>
		<num>Wildcard-test-2</num>
		<query>
		  <formula id="f1.0">
	<m:math alttext="X+?x0=?x1" display="inline">
		<m:mrow>
			<m:mi>x</m:mi>
			<m:mo>+</m:mo>
			<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x0"/>
			<m:mo>=</m:mo>
			<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x1"/>
		</m:mrow>
	</m:math>		
		  </formula>
		</query>
	</topic>
	<topic>
		<num>Wildcard-test-3</num>
		<query>
		  <formula id="f1.0">
	<m:math alttext="?x0^2+?x0+?x1" display="inline">
		<m:mrow>
			<m:msup>
				<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x0"/>
				<m:mn>2</m:mn>
			</m:msup>
			<m:mo>+</m:mo>
			<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x0"/>
			<m:mo>+</m:mo>
			<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x1"/>
		</m:mrow>
	</m:math>		
		  </formula>
		</query>
	</topic>
	<topic>
		<num>Wildcard-test-4</num>
		<query>
		  <formula id="f1.0">
	<m:math alttext="F(?x0)=\frac{?x1}{?x2}" display="inline">
		<m:mrow>
			<m:mi>F</m:mi>
            <m:mrow>
                <m:mo>(</m:mo>
                <mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x0"/>
                <m:mo>)</m:mo>
            </m:mrow>
			<m:mo>=</m:mo>
			<m:mfrac>
				<m:mrow>
					<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x1"/>
				</m:mrow>
				<m:mrow>
					<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x2"/>
				</m:mrow>
			</m:mfrac>
		</m:mrow>
	</m:math>		
		  </formula>
		</query>
	</topic>
	<topic>
		<num>Wildcard-test-5</num>
		<query>
		  <formula id="f1.0">
	<m:math alttext="F(?x0)=\frac{?x1}{?x2}" display="inline">
		<m:mrow>
			<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x0"/>
			<m:mo>=</m:mo>
            <m:mrow>
                <m:mo>{</m:mo>
                <mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x1"/>
