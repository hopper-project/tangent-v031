            </m:mrow>
		</m:mrow>
	</m:math>		
		  </formula>
		</query>
	</topic>
	<topic>
		<num>Wildcard-test-6</num>
		<query>
		  <formula id="f1.0">
	<m:math alttext="A=[?x0][?x1]" display="inline">
		<m:mrow>
		    <m:mi>A</m:mi>
		    <m:mo>=</m:mo>
		    <m:mrow>
		        <m:mo>[</m:mo>	
					<m:mtable>
						<m:mtr>
							<m:mtd>
								<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x0"/>
							</m:mtd>
						</m:mtr>			       
					</m:mtable>
		         <m:mo>]</m:mo>
		    </m:mrow>
			<m:mrow>
		        <m:mo>[</m:mo>	
					<m:mtable>
						<m:mtr>
							<m:mtd>
								<mws:qvar xmlns:mws="http://search.mathweb.org/ns" name="x1"/>
							</m:mtd>
